#include <stdio.h>

      int main(){
            int a, b;
            scanf("%d %d", &a, &b);
      int tong, hieu, tich, thuong;
            tong = a + b;
            hieu = a - b;
            tich = a * b;
            thuong = a / b;
            printf("%d %d %d %d", tong, hieu, tich, thuong);
            return 0;
      }
