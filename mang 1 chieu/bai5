#include <stdio.h>

int main(){
      int n, x;
      scanf("%d", &n);
      int a[n];
      int dem = 0;
      for(int i = 0; i < n; i++){
            scanf("%d", &a[i]);
      }
      scanf("%d", &x);
      for(int i = 0; i < n; i++){
            if(a[i] == x)
                  ++dem;
      }
      printf("%d", dem);
}
